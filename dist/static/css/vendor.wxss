.star .star-item {
  font-size: 0;
  display: inline-block;
  background-repeat: no-repeat;
}
.star.star-48 .star-item {
  width: 40rpx;
  height: 40rpx;
  margin-right: 44rpx;
  background-size: 40rpx 40rpx;
}
.star.star-48 .star-item:last-child {
  margin-right: 0;
}
.star.star-48 .star-item.on {
  background-image: url(data:image/png;base64,);
}
